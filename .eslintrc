{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "rules": {
    "semi": "error",
    "no-debugger": "off",
    "no-console": "off",
    "strict": ["error", "global"],
    "no-undef": "off",
    "no-case-declarations": "off",
    "no-unused-vars": "off",
    "react/prop-types": "off",
    "react/no-children-prop": "off",
    "react/no-unescaped-entities": "off"
  }
}